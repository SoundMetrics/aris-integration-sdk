<Window x:Class="WpfTestBench.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dv="clr-namespace:SoundMetrics.DataVisualization;assembly=SoundMetrics.DataVisualization"
        xmlns:local="clr-namespace:WpfTestBench"
        mc:Ignorable="d"
        Title="WPF Test Bench" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel MinHeight="16"/>
        <dv:ArisImageControl Grid.Row="1" Background="BlanchedAlmond"/>

    </Grid>
</Window>
